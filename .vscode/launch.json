{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Launch TUI Dev Mode",
            "type": "python",
            "request": "launch",
            "module": "cocli.tui.app",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "TEXTUAL_DEV": "true",
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "${workspaceFolder}/.venv/bin/python"
        },
        {
            "name": "Python: Launch Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Launch: achieve-goal emails=10,000 proximit=15",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/cocli",
            "python": "${workspaceFolder}/.venv/bin/python",
            "args": [
                "campaign",
                "achieve-goal",
                "turboship",
                "--emails",
                "10000",
                "--cloud-queue",
                "--proximity",
                "15",
                "--debug",
                "--panning-distance",
                "3"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Launch cocli campaign prospects to-hubspot-csv",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/cocli",
            "python": "${workspaceFolder}/.venv/bin/python",
            "args": ["campaign", "prospects", "to-hubspot-csv"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Launch cocli campaign prospects enrich",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/cocli",
            "python": "${workspaceFolder}/.venv/bin/python",
            "args": [
                "campaign",
                "prospects",
                "enrich",
                "--runner",
                "local",
                "--workers",
                "1"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Launch Custom Program with Args",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/path/to/your_script.py",
            "args": ["--param1", "value1", "--param2", "value2"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "cocli fz",
            "type": "python",
            "request": "launch",
            "module": "cocli.main",
            "args": [
                "fz"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: cocli import-data Debug",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "cocli.main",
            "args": [
                "import-data",
                "lead-sniper",
                "/home/mstouffer/.local/share/cocli_data/scraped_data/lead-sniper-photographer-20250823225603.csv",
                "--debug"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Behave Test",
            "type": "debugpy",
            "request": "launch",
            "module": "behave",
            "args": [
                "features/cli_help.feature"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: cocli scrape-shopify-myip Debug",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "cocli.main",
            "args": [
                "scrape-shopify-myip",
                "23.227.38.32",
                "--debug"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: cocli enrich-shopify-data Debug",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "cocli.main",
            "args": [
                "enrich-shopify-data"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Pytest Debug Scraper",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "tests/test_google_maps_scraper.py::test_scrape_google_maps_returns_data"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/.venv/bin/python"
        },
        {
            "name": "Debug: Google Maps Scraper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tests/debug_google_maps_scraper.py",
            "args": [
                "--search", "photographer",
                "--zip-code", "90210",
                "--max-results", "1",
                "--headed",
                "--devtools"
            ],
            "console": "integratedTerminal",
            "env": {
                "PWDEBUG": "0"
            },
            "justMyCode": true,
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}
